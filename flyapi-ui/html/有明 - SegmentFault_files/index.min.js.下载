low"],function(t,e){"user strict";t(".tagfollow").each(function(){var i;i=t(this),e({element:i,url:"/api/tag",successFn:function(t){i.siblings(".follows").text(t.data)}})})}),define("911",["main"],function(t){"use strict";var e,i,n,a,o,r,s,l,d,u,c,h,m,p,f,_,g,y,v,b,M,w,k;o=$("#911"),n=$("#reportForm"),i=o.find('[data-role="base"]'),d=o.find('[data-model="type"]'),r=o.find('[data-model="returnMsg"]'),e=o.find('[data-role="back"]'),s=o.find('[data-role="submit"]'),l=o.find('[data-model="type"]');var x=o.find('[data-model="action"]');a=i.find('[data-model="list"]'),c="",h="";var L="";m="",u=[],w=function(t){var e;return null!=t.subReason&&(u[t.value]=t.subReason),e='<li class="radio">',e+="<label>",e+='<input name="reason"',e+=' type="radio" value="'+t.value+" - "+t.desc+'"',"duplicate"===t.short.split("-")[1]&&(t.short="duplicate"),"none-of-all"===t.short||"duplicate"===t.short?e+=' data-action="'+t.short+'"':t.hasOwnProperty("subReason")&&(e+=' data-action="subReason" data-reason='+JSON.stringify(t)),e+=">",e+=t.value+'：<span class="text-muted">'+t.desc+"</span>",e+="</label>",e+="</li>"},k=function(t){var e,i;return e=t.isAccepted?"[解决] ":"",i='<li class="radio">',i+="<label>",i+='<input name="q" type="radio" value="'+t.id+'">',i+='<a href="'+t.url+'" title="'+t.title+'" class="mr10" target="_blank"> ',i+=e+t.title,i+="</a>",i+='<span class="text-muted">'+t.answers+" 个回答</span>",i+="</label>",i+="</li>"},y=function(t,e){var i,n;if(null!=t)return i=function(){var i,a,o;for(o=[],i=0,a=t.length;i<a;i++)n=t[i],o.push(e(n));return o}().join(" ")},g=function(t,e){var i,n,a;return e?(n=" has-input",i='<input type="hidden" name="q">'):(n="",i=y(t.subReason,w)),a='<div class="overlay'+n+'">',e||(a+='<p><strong class="required">该问题'+t.value+"，理由是：</strong></p>"),e&&(a+='<input class="form-control" type="text" placeholder="请输入已有问题标题或网址" >'),a+='<ul class="list-unstyled" data-model="list">',a+=i,a+="</ul>",a+="</div>"},v=function(t){return'<textarea autocomplete="false" class="